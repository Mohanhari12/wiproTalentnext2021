import java.util.*;
class Month{
public static void main(String[] args){
	Scanner s=new Scanner(System.in);
	int a;
	a=s.nextInt();
     switch (a)
    {
    case 1:
        System.out.println ("The name of month number is January");
        break;
    case 2:
        System.out.println ("The name of month number is February");
        break;
    case 3:
        System.out.println ("The name of month number  is March");
        break;
    case 4:
        System.out.println ("The name of month number is April");
        break;
    case 5:
        System.out.println ("The name of month number  is May");
        break;
    case 6:
        System.out.println ("The name of month number  is June");
        break;
    case 7:
        System.out.println ("The name of month number  is July");
        break;
    case 8:
        System.out.println ("The name of month number  is August");
        break;
    case 9:
        System.out.println ("The name of month number  is September");
        break;
    case 10:
        System.out.println ("The name of month number is October");
        break;
    case 11:
        System.out.println ("The name of month number  is November");
        break;
    case 12:
        System.out.println ("The name of month number  is December");
        break;
        default:
            System.out.println ("You have entered an invalid number");
        }
}
}
