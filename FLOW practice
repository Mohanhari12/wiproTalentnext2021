import java.util.*;
class Flow{
public static void main(String[] args){
	int a;
	Scanner s=new Scanner(System.in);
	a=s.nextInt();
	System .out.println("ENTER THE NUMBER:");
	{
		if(a>0){
		System.out.println("no is positive ");
		}
		else if(a<0){
		System.out.println("no is negative ");
		}
		else
		System.out.println("no is zero ");
		}
	}
}
